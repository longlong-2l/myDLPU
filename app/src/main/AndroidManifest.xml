<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.surpassli.www.myapp">
    <!--<uses-sdk android:minSdkVersion="4"></uses-sdk>-->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />


    <application
        android:name="org.litepal.LitePalApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ui.LoginActivity"
            android:launchMode="singleTask"></activity>
        <activity
            android:name=".ui.Account.School_Roll_Activity"
            android:launchMode="singleTask"></activity>
        <activity
            android:name=".ui.Account.Course_Result_Activity"
            android:launchMode="singleTask"></activity>
        <activity
            android:name=".ui.More.About.About_us_activity"
            android:launchMode="singleTask"></activity>
        <activity
            android:name=".ui.More.About.MyAppIntroduceActivity"
            android:launchMode="singleTask"></activity>
        <activity
            android:name=".ui.Account.Level_Grade_Activity"
            android:launchMode="singleTask"></activity>
        <activity
            android:name=".ui.Account.Course_Table_Activity"
            android:launchMode="singleTask"></activity>

        <receiver android:name=".MainActivity$MyBroadcastReceiver">
            <intent-filter>
                <action android:name="tab" />
            </intent-filter>
        </receiver>
        <meta-data
            android:name="UMENG_APPKEY"
            android:value="58773045766613068b000585"></meta-data><!--友盟AppKey-->

        <meta-data
            android:name="dlpu"
            android:value="58773045766613068b000585"></meta-data><!--友盟AppKey-->
        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="${UMENG_CHANNEL_VALUE}"></meta-data><!--渠道号-->
    </application>

</manifest>